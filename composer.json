{
    "name": "serverfireteam/panel",
    "description": "Admin panel for Laravel applications (fork of serverfireteam/panel)",
    "keywords": ["LaravelPanel","Laravel", "Panel", "Crud", "Admin Panel","CMS","administrator", "admin", "backend"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lytedev/rapyd-laravel"
        }
